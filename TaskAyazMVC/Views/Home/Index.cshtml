@model List<TaskAyazMVC.Models.EmpModel>
@{
    ViewBag.Title = "Home Page";
}
<h1>Book Data</h1>
<table class="table table-bordered table-hover table-responsive">
    <tr>
        <th>Id</th>
        <th>Subject</th>
        <th>Book Name</th>
        <th>Publication</th>
        <th>Author</th>
    </tr>
    @foreach (var itm in Model)
    {
<tr>
    <td>@itm.Id</td>
    <td>@itm.Subject</td>
    <td>@itm.Book_Name</td>
    <td>@itm.Publication</td>
    <td>@itm.Author</td>
    <td><a href=" @Url.Action("Delete","Home",new { itm.Id })" class="btn btn-danger">Delete</a></td>
    <td><a href=" @Url.Action("Edit","Home",new { itm.Id })" class="btn btn-danger">Edit</a></td>
</tr>
    }
</table>


